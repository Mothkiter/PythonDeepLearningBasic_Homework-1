            }).mouseleave(function () {
                $(".newChangeBox").removeClass("on");
            })
        } else {
            //如果是移动端加载移动端的js和css
            $(".newChangeBox>span").click(function () {
                var index = $(".newChangeBox>span").index(this);
                $(".newChangeList").hide();
                $(".newChangeList").eq(index).show();
            })

        }

